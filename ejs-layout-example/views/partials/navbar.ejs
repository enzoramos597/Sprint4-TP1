<nav>
    <div class="navbar">
        <a href="/">
            <img src="/images/logo.png" alt="Logo" class="logo">
        </a>
        <ul>
            <% navbarLinks.forEach(function(link) { %>
                <li>
                    <a href="<%= link.href %>">
                        <img src="<%= link.icon %>" alt="<%= link.text %> Icon">
                        <%= link.text %>
                    </a>
                </li>
            <% }); %>
        </ul>        
    </div>
</nav>

